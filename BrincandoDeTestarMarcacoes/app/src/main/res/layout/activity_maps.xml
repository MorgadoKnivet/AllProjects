<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"

    android:layout_width="match_parent"
    android:layout_height="match_parent"

   >

    <fragment xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:map="http://schemas.android.com/apk/res-auto"
        xmlns:tools="http://schemas.android.com/tools"
        android:id="@+id/map"
        android:name="com.google.android.gms.maps.SupportMapFragment"
        android:layout_width="319dp"
        android:layout_height="518dp"
        tools:context="com.example.moorg.brincandodetestarmarcacoes.MapsActivity"
        map:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="173dp"
        android:layout_marginRight="0dp"
        map:layout_constraintRight_toRightOf="parent"
        android:layout_marginLeft="0dp"
        map:layout_constraintLeft_toLeftOf="parent"
        map:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginBottom="8dp"
        map:layout_constraintVertical_bias="0.936"
        map:layout_constraintHorizontal_bias="1.0" />

    <Button



        android:id="@+id/local"
        android:layout_width="104dp"
        android:layout_height="32dp"
        android:text="Dados"
        android:textSize="10dp"
        android:layout_marginStart="49dp"
        android:layout_marginLeft="49dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="0dp" />
      <!--  android:onClick="irParaPontoCerto" -->




    <Button
        android:onClick="irParaTarefas"
        android:id="@+id/imageButton2"
        android:text="Tarefas"
        android:textSize="10dp"
        android:layout_width="101dp"
        android:layout_height="34dp"
        app:srcCompat="?attr/actionModeCloseDrawable"
        android:layout_marginRight="0dp"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintLeft_toRightOf="@+id/local"
        android:layout_marginLeft="8dp"
        app:layout_constraintHorizontal_bias="0.017"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="0dp" />



</android.support.constraint.ConstraintLayout>